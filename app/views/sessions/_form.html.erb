<%= form_with url: sign_in_path do |form| %>
  <div class='form-group'>
    <%= form.label :username %>
    <%= form.text_field :username, class: 'form-entry' %>
  </div>
  <div class='form-group'>
    <%= form.label :password %>
    <%= form.password_field :password, class: 'form-entry' %>
    <p id='notice'><%= notice %></p>

  </div>
  <%= submit_tag 'Sign In', class: 'btn-hollow' %>
<% end %>
